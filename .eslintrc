{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "extends": "airbnb",
    "rules": {
        "curly": "error",
        "no-console": 0,
        "no-param-reassign": 0,
        "class-methods-use-this": 0,
        "no-await-in-loop": 0
    }
}
